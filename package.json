{
  "name": "react-native-blockommerce",
  "version": "0.1.0",
  "description": "The first-class Framework high-performance e-commerce for Mobile",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-blockommerce.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/rafygonzalez/react-native-blockommerce",
  "author": "Rafael Gonzalez <rafygonzalez089@gmail.com> (https://github.com/rafygonzalez)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafygonzalez/react-native-blockommerce/issues"
  },
  "homepage": "https://github.com/rafygonzalez/react-native-blockommerce#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@formatjs/intl-datetimeformat": "^5.0.0",
    "@formatjs/intl-getcanonicallocales": "^1.9.0",
    "@formatjs/intl-locale": "^2.4.45",
    "@formatjs/intl-numberformat": "^7.4.2",
    "@formatjs/intl-pluralrules": "^4.3.2",
    "@hookform/resolvers": "^2.8.8",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native-community/netinfo": "^8.0.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.66.15",
    "@types/react-native-vector-icons": "^6.4.10",
    "commitlint": "^16.1.0",
    "cross-fetch": "^3.1.5",
    "debounce": "^1.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.flatten": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isregexp": "^4.0.1",
    "lodash.isstring": "^4.0.1",
    "lodash.merge": "^4.6.2",
    "pod-install": "^0.1.31",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-hook-form": "^7.26.1",
    "react-intl": "^5.24.6",
    "react-native": "0.67.2",
    "react-native-builder-bob": "^0.18.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-maps": "0.30.1",
    "react-native-modal-datetime-picker": "^13.0.1",
    "react-native-offline": "^6.0.0",
    "react-native-permissions": "^3.2.0",
    "react-native-reanimated": "2.4.1",
    "react-native-redash": "16.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.0",
    "react-native-select-dropdown": "^1.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-native-webview": "^11.17.2",
    "release-it": "^14.12.4",
    "swr": "1.2.1",
    "typescript": "^4.5.5",
    "yup": "^0.32.11",
    "lottie-react-native": "^5.0.1",
    "react-native-device-info": "^8.4.9"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "@react-native-community/datetimepicker": "*",
    "@react-native-community/netinfo": "*",
    "@react-navigation/bottom-tabs": "*",
    "@react-navigation/drawer": "*",
    "@react-navigation/native": "*",
    "@react-navigation/native-stack": "*",
    "@react-navigation/stack": "*",
    "react": "*",
    "react-native": "*",
    "react-native-fast-image": "*",
    "react-native-geolocation-service": "*",
    "react-native-gesture-handler": "*",
    "react-native-maps": "*",
    "react-native-modal-datetime-picker": "*",
    "react-native-offline": "*",
    "react-native-permissions": "*",
    "react-native-reanimated": "*",
    "react-native-redash": "*",
    "react-native-safe-area-context": "*",
    "react-native-screens": "*",
    "react-native-select-dropdown": "*",
    "react-native-svg": "*",
    "react-native-vector-icons": "*",
    "react-native-webview": "*",
    "lottie-react-native": "*",
    "react-native-device-info": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "alias": {
    "$commerce": "./src/framework/local",
    "$styleguide": "./src/framework/styleguide",
    "$engine": "./src/framework/engine",
    "$core-commerce": "./src/framework/commerce"
  }
}
